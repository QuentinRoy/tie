{
  "name": "tie-constraint",
  "version": "0.3.1",
  "description": "A tiny constraint library.",
  "main": "dist/tie.js",
  "module": "lib/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test babel-node tests | ./node_modules/.bin/tap-spec",
    "browser-test": "cross-env NODE_ENV=test ./node_modules/.bin/karma start karma.conf.js",
    "build-dev": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --config webpack.development.js",
    "build-prod": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config webpack.production.js",
    "build": "yarn build-dev && yarn build-prod"
  },
  "keywords": [
    "constraint",
    "events"
  ],
  "author": "Quentin Roy <quentin@quentinroy.fr>",
  "directories": {
    "example": "demos",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QuentinRoy/tie.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.4.5",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "cross-env": "^5.1.3",
    "deep-equal": "^1.0.1",
    "eslint": "^4.18.2",
    "events": "^2.0.0",
    "karma": "^2.0.0",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-tap": "^4.1.3",
    "karma-webpack": "^2.0.13",
    "tap-spec": "^4.1.1",
    "tape": "^4.2.2",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "babel-preset-env": "^1.6.1"
  }
}
